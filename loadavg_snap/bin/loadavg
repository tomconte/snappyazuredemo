#!/bin/bash

machine=`uname -m`
case "$machine" in
  x86_64)
    triplet=x86_64-linux-gnu
  ;;
  armv7l)
    triplet=arm-linux-gnueabihf
  ;;
  *)
    echo "Unsupported uname -m: $machine" >&2
    exit 1
  ;;
esac

export LD_LIBRARY_PATH=${SNAPP_APP_PATH}/lib/${triplet}

${SNAPP_APP_PATH}/bin/${triplet}/eventhub_demo
